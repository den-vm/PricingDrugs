    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <!-- Google web fonts -->
    <link href="~/css/JVNLP/Font.css" rel="stylesheet" />
    <link href="~/css/JVNLP/Form.css" rel="stylesheet" />
    <link rel="shortcut icon" href="">
</head>
<body>
    <form id="upload" action="" method="post" enctype="multipart/form-data" name="collection">
